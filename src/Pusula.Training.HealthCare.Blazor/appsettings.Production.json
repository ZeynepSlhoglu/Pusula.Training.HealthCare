{
  "App": {
    "SelfUrl": "https://localhost:44398",
    "RedirectAllowedUrls": "https://localhost:44398"
  },
  "ConnectionStrings": {
    "Default": "Host=postgres;Port=5432;Database=db-name;User ID=postgres;Password=your-password;"
  },
  "AuthServer": {
    "Authority": "https://localhost:44398",
    "RequireHttpsMetadata": false
  },
  "StringEncryption": {
    "DefaultPassPhrase": "your-key"
  },
  "ElasticSearch": {
    "Url": "http://localhost:9200"
  },
  "Redis": {
    "Configuration": "localhost:6379,Password=your-password,abortConnect=false"
  },
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "localhost",     
        "UserName": "user-name",
        "Password": "user-password",
        "Port": 5672
      }
    },
    "EventBus": {
      "ClientName": "client-name",
      "ExchangeName": "PTH"
    },
    "Cluster": {
      "Nodes": [
        "rabbit@rabbitmq-node-1",       
        "rabbit@rabbitmq-node-2",
        "rabbit@rabbitmq-node-3"
      ]
    }
  },
  "Syncfusion":{
    "LicenseKey": "your-syncfusion-key"
  }
}
